{
  "name": "angular2-node",
  "version": "1.0.0",
  "description": "Angular and NodeJS template application",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "rimraf public && webpack --config config/webpack.dev.js",
    "build:prod": "rimraf public && webpack --config config/webpack.prod.js --bail",
    "clean": "rimraf npm*.log* && rimraf public",
    "start:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "start": "npm run start:dev",
    "test": "karma start karma.webpack.config.js"
  },
