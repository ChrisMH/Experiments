{
  "name": "betacon",
  "version": "1.0.0",
  "description": "BetaCon application",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "rimraf public && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "rimraf public && webpack --config config/webpack.prod.js  --progress --profile --bail",
    "clean": "rimraf npm*.log* && rimraf public",
    "start:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch",
