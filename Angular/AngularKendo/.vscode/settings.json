// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "npm-debug*": true,
        "node_modules": true,
        "**/*.js.map": true,
        "**/*.css.map": true,
        "**/*.js": { "when" : "$(basename).ts" },
        "**/*.css": { "when" : "$(basename).styl" }
    },
    "vsicons.presets.angular": true
}
