// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "npm-debug*": true,
        "**/*.js": { "when" : "$(basename).ts" },
        "**/**.js": { "when" : "$(basename).tsx" },
        "**/*.css": { "when" : "$(basename).styl" }
    }
}